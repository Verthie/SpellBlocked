; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SpellBlocked"
config/tags=PackedStringArray("2d", "platformer", "project", "puzzle")
run/main_scene="res://scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

EventBus="*res://globals/event_bus.gd"
AudioManager="*res://globals/audio_manager.tscn"
SceneSwitcher="*res://globals/scene_switcher.tscn"
BgmManager="*res://globals/bgm_manager.tscn"
TransitionManager="*res://globals/transition_manager.tscn"
Globals="*res://globals/globals.gd"
SaveDataManager="*res://globals/save_data_manager.gd"
InterfaceCursor="*res://globals/interface_cursor.tscn"
Cursor="*res://globals/cursor.tscn"

[debug]

gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/Open External Editor/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/animations/": "gray",
